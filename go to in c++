#include<iostream>
using namespace std;
int main()
{
    int num;
    for(int i=0;i<10;i++)
    {
        cout<<i<<endl;
        for(int j=0;j<10;j++)
        {
cout<<"enter the number "<<endl;
cin>>num;
if(num==0)
{
    goto end;
}
        }
        end:
        {
            cout<<"you have enter zero"<<endl;
        }
    }
}
